{
  "name": "Outline",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon32.png",
    "default_title": "Outline current page"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
		"persistent": false,
		"scripts": [
			"index.js"
		]
	}
}
